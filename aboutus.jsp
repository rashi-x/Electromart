       <!--SCROLL REVEAL EFFECT  -->
       <script>
           window.sr = ScrollReveal();

           sr.reveal('.btn', {
               duration: 2000,
               origin: 'top',
               distance: '60px',
           });

           sr.reveal('#bio', {
               duration: 2000,
               origin: 'bottom',
               distance: '-30px',
               viewFactor: 0.1,
               reset: true
           });
           
           sr.reveal('#testimonial', {
               duration: 2000,
               origin: 'left',
               distance: '20px',
               viewFactor: 0.1,

               reset: true
           });
           
           sr.reveal('#elsalvador', {
               duration: 2000,
               origin: 'bottom',
               viewFactor: 0.1,
               reset: true
           });
           
           sr.reveal('#therose', {
               duration: 2000,
               origin: 'bottom',
               viewFactor: 0.1,
               reset: true
           });
           
           sr.reveal('#thecouple', {
               duration: 2000,
               origin: 'bottom',
               viewFactor: 0.1,
               reset: true
           });
           
           sr.reveal('#pao', {
               duration: 2000,
               origin: 'right',
               distance: '100px',
               viewFactor: 0.1,
               reset: true
           });
       </script>
       <script>
            $(document).ready(function() {
                
                $('body').prepend('<a href="#bio" class="btn btn-circle back-to-top"><i class="fa fa-angle-double-up"></i></a>'); 
              
                $(window).scroll(function() {
                   if ($(this).scrollTop() > 580) {
                         $('.back-to-top').fadeIn('slow');
                    } else {
                        $('.back-to-top').fadeOut();
                    }
                });
            });
       </script>
       
